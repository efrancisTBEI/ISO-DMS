        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ISO_DMS"
        mc:Ignorable="d"
        Title="ScanDocuments" Height="200" Width="300" WindowStyle="ToolWindow">
    <Grid>
        <Button x:Name="btnScan" Content="Button" HorizontalAlignment="Left" Height="47" Margin="105,96,0,0" VerticalAlignment="Top" Width="84" Click="btnScan_Click" Style="{DynamicResource StyleButton}"/>

    </Grid>
</Window>
